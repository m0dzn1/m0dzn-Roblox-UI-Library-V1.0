local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/m0dzn1/m0dzn-ui-lib-test/refs/heads/main/m0dzn%20ui%20lib"))()

if not Library then return end

-- You only need to define Title and Default Keybind now
local Window = Library:CreateWindow({
    Title = "M0dzn Hub Premium",
    Keybind = Enum.KeyCode.M -- You can change Clickgui keybind
})

-- Just add your MAIN features. Settings is auto-added.
local Main = Window:Tab("Main")

Main:Section("Player")
Main:Button("WalkSpeed", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
end)

Window:Notification("Script Loaded!")
